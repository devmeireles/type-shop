{
  "name": "ts-shop",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
    "start": "node dist/server.js",
    "start:dev": "ts-node-dev --respawn --pretty --transpile-only src/server.ts",
