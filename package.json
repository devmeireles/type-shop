    "test:coverage": "jest --projects ./test --runInBand --coverage",
    "style:fix": "prettier --write 'src/**/*.ts'",
    "lint:fix": "eslint ./src --ext .ts --fix",
    "typeorm": "npx ts-node ./node_modules/typeorm/cli.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "^7.13.10",
