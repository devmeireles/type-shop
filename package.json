    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
